  <link rel="stylesheet" href='{{url "/css/main.css"}}' type="text/css">

  <script src="https://kit.fontawesome.com/2551d62386.js" crossorigin="anonymous"></script>
</head>
<body class="d-flex flex-column min-vh-100">
{{block "header" .}}
{{end}}

